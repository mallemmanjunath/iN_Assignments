   "source": [
    "#creating a required pattern\n",
    "\n",
    "n=5\n",
    "for i in range(n):\n",
    "    print(\"*\"*(i+1)+\" \"*(n-1))\n",
    "for j in range(n-1):\n",
    "    print(\"*\"*(n-1-j)+\" \"*(j+1))"
   ]
